    <style name="Healthy.Text.Body0.Regular">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/color_meal_nutrient_value</item>
    </style>

    <style name="Healthy.Text.Body0">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/color_black</item>
    </style>

    <style name="Healthy.Text.Body1">
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#DFDFDF</item>
    </style>

    <style name="Healthy.Text.Body1.Regular">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Healthy.Text.Body1.Semibold">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/color_black</item>
    </style>

    <style name="Healthy.Text.Caption">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#DFDFDF</item>
