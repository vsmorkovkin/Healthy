    <style name="Healthy.Text.Caption1">
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/color_black</item>
    </style>

    <style name="ThemeOverlay.AddMealDialog" parent="ThemeOverlay.AppCompat">
        <item name="colorPrimary">@color/color_nutrition</item>
    </style>

    <style name="ThemeOverlay.AddWaterIntakeDialog" parent="ThemeOverlay.AppCompat">
        <item name="colorPrimary">@color/color_water</item>
    </style>

    <style name="ThemeOverlay.EnterSleepTimeDialog" parent="ThemeOverlay.AppCompat">
        <item name="colorPrimary">@color/color_sleep</item>
    </style>

    <style name="ThemeOverlay.EnterCurrentWeightDialog" parent="ThemeOverlay.AppCompat">
